{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Jabuk√≥d",
	"patterns": [
		{
			"include": "#block_comment"
		},
		{
			"include": "#line_comment"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#type"
		},
		{
			"include": "#storage"
		},
		{
			"include": "#truefalse"
		},
		{
			"include": "#function"
		},
		{
			"include": "#variable"
		}
	],
	"repository": {
		"block_comment": {
			"name": "comment.block.jabukod",
			"begin": "/\\*",
			"end": "\\*/"
		},
		"line_comment": {
			"name": "comment.line.double-slash.jabukod",
			"match": "//.*$"
		},
		"strings": {
			"name": "string.quoted.double.jabukod",
			"begin": "\"",
			"end": "\"",
			"patterns": [{
				"name": "constant.character.escape.jabukod",
				"match": "\\\\."
			}]
		},
		"numbers": {
			"patterns": [{
				"name": "constant.numeric.jabukod",
				"match": "(i?)-?\\b(0x[0-9a-f]+|\\d+\\.\\d+(e-?\\d+)?|\\d+(e-?\\d+)?)\\b"
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.jabukod",
				"match": "\\b(if|else|while|for|foreach|return|exit|suspend|resume|continue|break|redo|restart|read|write)\\b"
			}]
		},
		"operators": {
			"patterns": [{
				"name": "keyword.operator.jabukod",
				"match": "\\b(\\*\\*|-|\\~|!|\\*|\\/|%|\\+|<<|>>|<|<=|>|>=|==|!=|&|\\^|\\||&&|\\|\\||=)\\b"
			}]
		},
		"type": {
			"patterns": [{
				"name": "storage.type.jabukod",
				"match": "\\b(int|float|bool|string|enum)\\b"
			}]
		},
		"storage": {
			"patterns": [{
				"name": "storage.modifier.jabukod",
				"match": "\\b(const|static)\\b"
			}]
		},
		"truefalse": {
			"patterns": [{
				"name": "constant.language.jabukod",
				"match": "\\b(true|false)\\b"
			}]
		},
		"function": {
			"patterns": [{
				"name": "entity.name.function.jabukod",
				"match": "(i?)\\b[a-z_][a-z0-9_]*\\s*\\("
			}]
		},
		"variable": {
			"patterns": [{
				"name": "variable.other",
				"match": "(i?)\\b[a-z_][a-z0-9_]*\\b"
			}]
		}
	},
	"scopeName": "source.jabukod"
}
